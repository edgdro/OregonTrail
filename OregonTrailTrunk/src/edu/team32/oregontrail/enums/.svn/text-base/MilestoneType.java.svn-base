package edu.team32.oregontrail.enums;

/**
 * Represents the type of milestone.
 * 
 * @author Team 32
 *
 */
public enum MilestoneType {
	/**
	 * Used for denoting the start town, aka Independence
	 */
	START_TOWN, 
	/**
	 * Used for denoting milestones that are rivers
	 */
	RIVER, 
	/**
	 * Used for denoting milestones that have stores
	 */
	FORT, 
	/**
	 * Used for denoting milestones that are just landmarks, such as rocks
	 */
	LANDMARK, 
	/**
	 * Used for denoting Oregon
	 */
	OREGON;
}
